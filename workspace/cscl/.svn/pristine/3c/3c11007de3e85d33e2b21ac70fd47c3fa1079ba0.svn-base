.jumbotron{
	background-color:transparent !important; 
}

.row{
	margin:30px 0px ;
}

/*表单*/
.main{
	background: #ffffff !important;
	overflow: hidden;
}

.com_img{background:url(images/register_iden/company.png) no-repeat 2px}
.iden_img{background:url(images/register_iden/iden.png) no-repeat 2px}
#iden{cursor: pointer;}
/*验证提示样式*/
.form_warp{position: relative;}
#comName_err{
	position: absolute;
	top:30px;
	left: 50px;
	
}

/*图片缩略图*/
#img1,#img2,#img3{
	width:210px;
	height:140px;
}

/*按钮*/
.btn_next{
	color: #fff !important;
	border: 1px;
	border-color: #ddd;
	width: 49%;
	height: 30px;
	line-height: 18px;
}
@media only screen and (max-width:970px ) {
	.btn_next{
		width: 100%;
	}
}
@media only screen and (min-width: 768px) {
	.btn_ignore,.btn_next{height: 50px; line-height: 40px;}
	.btn_ignore{float: left;}
	.btn_next{float: right;}
}
.btn_ignore,.btn_ignore:hover{
	color: #fff !important;
	background: #8C8C8C;
}
.btn_next:hover{
	color: #fff !important;
}
