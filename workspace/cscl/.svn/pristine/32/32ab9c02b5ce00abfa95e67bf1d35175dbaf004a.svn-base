@charset "utf-8";
body{
	background-color: #f0f2f5;
}
#myTab a{
	text-align: center;
}
#myTab a:hover{
	color: #E64249 !important;
	background-color:#EEEEEE !important;
	border:none !important;
	border-bottom: 2px solid #E64249 !important;
	border-top:1px solid transparent!important;
}
/*大于768px*/
@media screen and (min-width: 769px) {
	.input_left
	{
		margin-left: 42px;
	}
	/*删除按钮*/
	.deletebtn
	{
		margin-top: 5px;
	}
}
/*小于768px*/
@media screen and (max-width: 769px) {

	/*表单位移*/
	.input_left
	{
		margin-left: 0px;
	}
	/*删除图片大小*/
	.deletebtn img
	{
		width: 25px;
		margin-top: 2px;
	}

}
/*小于455px*/
@media (min-width: 329px) and (max-width: 455px) {

	.items_type li 
	{

		width: 50%;
		height: 100px!important;
		margin-top: 0%!important;
	}
	.items_type li img
	{
		width: 90%;
	}


}
/*小于330px*/
@media screen and (max-width: 330px) {

	.items_type li 
	{

		width: 100%;
		margin-top: 6%!important;
	}
	.car_border
	{
		height: 745px!important;
	}

}
/* 小屏幕（平板，大于等于 768px） */
/*@media (min-width: @screen-sm-min) { }*/

/* 中等屏幕（桌面显示器，大于等于 992px） */
/*@media (min-width: @screen-md-min) { ... }*/

/* 大屏幕（大桌面显示器，大于等于 1200px） */
/*@media (min-width: @screen-lg-min) {}*/
.container_title
{
	margin-top: 1%;
	margin-bottom: 0;
	padding:20px 0;
}
.activedown
{
	background-color: #e6454a!important;
	border: none!important;
	display: block;
	border-radius: 4px!important;
	z-index: 4;
}
/*内容背景*/
.container_tem
{
	background-color: #fff;
	margin-bottom: 10%;
	overflow:hidden;
}
.icons{
	margin-left:10px;
}
.btn_colorXie img
{
	width: 18px;
	vertical-align: sub;
}
.car_mame img
{
	width: 18px;
	vertical-align: middle;
}
/*零担下移*/
.lin_car
{
	margin-top: 1%;
}
.car_Smame
{
	margin-left: 0px;
}
.car_mame 
{
    font-size: 18px;
    padding-top: 1%;
    padding-bottom: 1%;
}
.deletebtn
{
	visibility: hidden;
	z-index: 5;
	cursor: pointer;
}
/*红色字体*/
.red_span
{
	color: #e6454a;
}
.items_type
{
	margin-left: -40px;
}
.items_type li 
{

	height: 105px;
	list-style: none;
	padding: 0% 0%;
	margin-top: 2%;
}
.items_type li img
{
	display: block;
	margin: auto;
	cursor: pointer;
}
.items_type li p
{
	text-align: center;
	margin-top: 3%;
	color: #5c6068;
}
/*外边框*/
.car_border
{
	margin-top: 1%;
	padding: 3% 3%;
	border: 1px solid #ddd;
	overflow: hidden;
}
/*字体颜色*/
.color_text
{
	color: #999999;
}
/*车型选择位置*/
.car_models
{
	margin: 1% 0%;
}
.models_btn button
{
	margin-bottom: 5%;
}
.form-control-feedback
{
	top: 20px;
	right: 30px;
	width: 50px;
	text-align: right;
	pointer-events:auto!important;
	z-index: 999;
}
/*按钮*/
.red_btn
{
	margin-top: 3%;
	margin-bottom: 10%;
	background-color: #e6454a;
}
/*下拉菜单*/
.piao
{
	z-index: 999;
	cursor: pointer;
}
#ul_down
{
	border-radius: 4px;
	border: 1px solid #ddd;
	list-style: none;
	margin-top: 1%;
	padding: 0;
}
#ul_down li
{
	width: 100%;
	display: block;
	cursor: pointer;
	text-align: center;
}
#ul_down li:hover
{
	background-color: #ddd;
}
/*按钮换色*/
.lin_color_btn
{
	border: 1px solid #E6454A!important;
	color: #E6454A!important;
}
.btn:focus{
	outline:none;
	background-color:#FFFFFF;
}

/*增值服务*/
.collectingFees{
	border:none;
	width:50%;
	height:28px;
	margin:-20px auto;
	display:inline;
}
.collection{
	padding:0 1.9% !important;
}
@media only screen and (max-width: 768px) {
	.collection{padding:0 2.1% !important;}
}
@media only screen and (max-width: 600px) {
	.collection{padding:0 2.6% !important;}
}
@media only screen and (max-width: 480px) {
	.collection{padding:0 3.7% !important;}
}

/*提交订单*/
.submit_form{
	background-color:#DCDCDC;
	margin-top:45px;
}
.submit_form p{
	font-size:24px;
	text-align:center;
	color:#5B5F67;
	font-weight:bold;
	
}
.submit_form p span{
	font-size:48px;
	color:#E6454A;
	font-weight:bold;
	
}
.submit_form button{
	margin-top:48px;
}
.submit_form button:focus{
	background-color:#E54549;
}
.mileage{
	line-height:126px;
}
.cost{
	margin-top:20px;
}
@media only screen and (max-width: 992px) {
	.mileage{line-height:50px;}
	.cost{margin-top:0;}
	.submit_form button{margin-top:10px;margin-bottom:20px;}
}

/*验证信息提示*/
#time_err,#time2_err,#contactName_err,#contactPhone_err,#contactName2_err,#contactPhone2_err,#goodsLength_err,#goodsNumber_err,#goodsVolume_err{
	position: relative;
	top:-16px;
}
#address_th_err,#address_xh_err{
	position: relative;
	top:-31px;
}
#time3_err,#time4_err,#lin_contactName_err,#lin_contactPhone_err,#lin_xh_contactName_err,#lin_xh_contactPhone_err,#goodsLength2_err,#goodsNumber2_err,#goodsVolume2_err{
	position: relative;
	top:-16px;
}
#lin_address_th_err,#lin_address_xh_err{
	position: relative;
	top:-31px;
}
#goodweight_err,#goodsVolume2_err{
	position: relative;
	top:-16px;
}

/*鼠标变成手*/
.map_icon,.address_icon,#address_table tr{
	cursor:pointer;
}