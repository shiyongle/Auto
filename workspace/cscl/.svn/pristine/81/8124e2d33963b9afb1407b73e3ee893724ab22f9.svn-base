package com.pc.query.menuitem;

import java.io.Serializable;

import com.pc.query.aBase.BaseQuery;

public class MenuItemQuery extends BaseQuery  implements Serializable {

	private static final long serialVersionUID = 1123350423712642495L;
	private int urid;
	private String searchKey;
	
	public String getSearchKey() {
		return searchKey;
	}
	public void setSearchKey(String searchKey) {
		this.searchKey = searchKey;
	}
	public int getUrid() {
		return urid;
	}
	public void setUrid(int urid) {
		this.urid = urid;
	}

}
