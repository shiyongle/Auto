package com.pc.query.factory;

import java.io.Serializable;
import java.util.Date;

import com.pc.query.aBase.BaseQuery;

public class FactoryQuery extends BaseQuery implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 4786819674795861071L;
	
	private Integer factory_id;
	private String factory;//工厂名称
	private Integer fstatus;//状态：1启用，2停用
	private String flinkman;//联系人
	private String flinkphone;//联系电话
	private String faddress;//地址
	private String flongitude;//经度
	private String flatitude;//纬度
	private String fremark;//备注
	private Integer fcreator;//创建人
	private Date fcreat_time;//创建时间
	private Integer feditor;//修改人
	private Date fedit_time;//修改时间
	public Integer getFactory_id() {
		return factory_id;
	}
	public void setFactory_id(Integer factory_id) {
		this.factory_id = factory_id;
	}
	public String getFactory() {
		return factory;
	}
	public void setFactory(String factory) {
		this.factory = factory;
	}
	public Integer getFstatus() {
		return fstatus;
	}
	public void setFstatus(Integer fstatus) {
		this.fstatus = fstatus;
	}
	public String getFlinkman() {
		return flinkman;
	}
	public void setFlinkman(String flinkman) {
		this.flinkman = flinkman;
	}
	public String getFlinkphone() {
		return flinkphone;
	}
	public void setFlinkphone(String flinkphone) {
		this.flinkphone = flinkphone;
	}
	public String getFaddress() {
		return faddress;
	}
	public void setFaddress(String faddress) {
		this.faddress = faddress;
	}
	public String getFlongitude() {
		return flongitude;
	}
	public void setFlongitude(String flongitude) {
		this.flongitude = flongitude;
	}
	public String getFlatitude() {
		return flatitude;
	}
	public void setFlatitude(String flatitude) {
		this.flatitude = flatitude;
	}
	public String getFremark() {
		return fremark;
	}
	public void setFremark(String fremark) {
		this.fremark = fremark;
	}
	public Integer getFcreator() {
		return fcreator;
	}
	public void setFcreator(Integer fcreator) {
		this.fcreator = fcreator;
	}
	public Date getFcreat_time() {
		return fcreat_time;
	}
	public void setFcreat_time(Date fcreat_time) {
		this.fcreat_time = fcreat_time;
	}
	public Integer getFeditor() {
		return feditor;
	}
	public void setFeditor(Integer feditor) {
		this.feditor = feditor;
	}
	public Date getFedit_time() {
		return fedit_time;
	}
	public void setFedit_time(Date fedit_time) {
		this.fedit_time = fedit_time;
	}
	
	
}
