a:visited{
	color: #333;
	text-decoration: none;
}
a:link{
	color: #333;
	text-decoration: none;
}
.jumbotron{
	background: #fff;
	overflow: hidden;
}
.row{
	margin: 30px 0;
}
.demo{
	border-right:1px dotted #ddd ;
	height: 220px;
}
.demo:last-child{
	border-right:0;
}

@media only screen and (max-width:768px ) {
.demo{
		border: 0px;
		padding-top: 5px;
		border-bottom:1px dotted #ddd ;
		height:auto;
		
	}
}
.demo > div{
	margin-bottom: 20px;
}

.redNum{
	color: #e6454a;
	font-size: 34px;
}

.btn-a{
	width: 78px;
	height: 30px;
	line-height: 18px;
}

/*银行卡*/
.bank_card{
	width: 200px;
	height: 75px;
	border: 1px dotted #ddd;
	margin: 0 auto;
	margin-bottom:20px;
}
.img_bank{
	float: left;
	margin: 0 6px;
	margin-top: 15px;
}
.mess_bank{
	padding-top: 6px;
	line-height: 15px;
}

.mess_bank p{
	font-size: 15px;
}

.mess_bank p:last-child{
	color: #999999;
}


/*交易记录*/
.gray_bg{
	background: #f0f2f5;
	color: #5c6068;
	height: 46px;
	line-height: 46px;
}
.btn-a{
	width: auto !important;
}



@media only screen and (min-width:768px ) {
	#time1,#time2{width:100px;}	
}

/*表格*/
a.blue{
	color: #00aaee !important;
}
a.blue:hover{
	color: #00aaee !important;
}

table tr td{
	height: 50px;
	line-height: 50px !important;
}
