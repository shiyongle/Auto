<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.model.storebalance.Storebalance" table="t_inv_storebalance" >
        <id name="fid" type="string">
            <column name="FID" length="45" />
            <generator class="assigned" />
        </id>
        <property name="fcreatorid" type="string">
            <column name="fcreatorid" length="44" />
        </property>
        <property name="fcreatetime" type="timestamp">
            <column name="fcreatetime" length="0" />
        </property>
        <property name="fupdateuserid" type="string">
            <column name="fupdateuserid" length="44" />
        </property>
        <property name="fupdatetime" type="timestamp">
            <column name="fupdatetime" length="0" />
        </property>
        <property name="fcustomerid" type="string">
            <column name="fcustomerid" length="44" />
        </property>
        <property name="fproductId" type="string">
            <column name="fProductID" length="44" />
        </property>
        <property name="finqty" type="java.lang.Integer">
            <column name="finqty" />
        </property>
        <property name="foutqty" type="java.lang.Integer">
            <column name="foutqty" />
        </property>
        <property name="fbalanceqty" type="java.lang.Integer">
            <column name="fbalanceqty" />
        </property>
        <property name="fwarehouseId" type="string">
            <column name="FWarehouseID" length="36" />
        </property>
        <property name="fwarehouseSiteId" type="string">
            <column name="FWarehouseSiteID" length="36" />
        </property>
        <property name="fdescription" type="string">
            <column name="fdescription" length="200" />
        </property>
        <property name="fsaleorderid" type="string">
            <column name="fsaleorderid" length="44" />
        </property>
        <property name="forderentryid" type="string">
            <column name="forderentryid" length="44" />
        </property>
        <property name="fproductplanId" type="string">
            <column name="fproductplanID" length="44" />
        </property>
        
        <property name="fsupplierID" type="string">
            <column name="FSUPPLIERID" length="44" />
        </property>
        
        <property name="ftype" type="java.lang.Integer">
            <column name="FTYPE" />
             </property>
         <property name="fallotqty" type="java.lang.Integer">
            <column name="fallotqty" />
        </property>
        <property name="ftraitid" type="string">
            <column name="ftraitid" length="44" />
        </property>
 
    </class>
</hibernate-mapping>
