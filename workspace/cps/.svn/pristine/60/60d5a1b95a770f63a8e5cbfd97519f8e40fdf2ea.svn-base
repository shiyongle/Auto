@CHARSET "UTF-8";
*{
	margin:0px;
	padding:0px;
}
input[readonly]{
	color: #888;
}
.title{
	font: 12px 宋体;
    height: 35px;
    line-height: 35px;
    background-color: #f1f1f1;
    padding-left:10px;
}
.hx{
	text-align:center;
	padding:20px;
}
.hx_l{
	float: left;
	border: none;
	margin-top: -7px;
}
.hx_r{
	margin-top: -1px;
	float: right;
	border: none;
}
.hx_title{
	font-family: 微软雅黑;
	font-size: 24px;
}
.addSchemeDesign{
	font: 18px 微软雅黑;
}
.addSchemeDesign span{
	margin-left:30px;
}
.addSchemeDesign img{
	vertical-align:middle;
	width:50px;
	height:50px;
    cursor: pointer;
}
.button{
	text-align:center;
}
.button ._btn{
	cursor:pointer;
	width:125px;
	height:45px;
	font-size:18px;
	background-color:#f57771;
	border:1px solid #CC0000;
	font-family:"微软雅黑";
	line-height:45px;
    margin-right: 5px!important;
	}
.button span.save{
	color:#fff;
	background-color:#f76350;
}
.button span.back{
	color:#fff;
	background-color:#b0b0b0;
}
.content{
	padding:20px;
	width:780px;
	margin-left:90px;
}
.content table{
	width:780px;
	text-align:center;
/* 	margin-left:90px; */
	background-color:#e5e5e5;
}
.content table tr{
	height:60px;
}
.content table input{
    width: 200px;
    height: 25px;
    float: left;
    outline: none;
    padding-left:5px;
}
.content table textarea{
	padding-left:5px;
	outline: none;
}
.del {
    margin: 0px 0 0 -10px;
    position: relative;
    background: url(../../../css/images/del.png) no-repeat scroll 0 0 transparent;
    width: 20px;
    float: right;
    height: 20px;
    overflow: hidden;
    text-indent: -9999px;
    top: 12px;
    right: -18px;
}
.titleTable,.a,.b{
	padding:10px;
	margin-left:20px;
	font: 22px 微软雅黑;
}
.titleTable tr,.titleDiv{
	height:80px;
}
.titleTable .imageTd{
	background-color:#e5e5e5;
	border:1px solid #bfbfbf;
}
span.redLine,.titleTable span.redLine{
	margin:0px;
	padding:0px;
	font:27px 微软雅黑;
	color:red;
	margin-right:5px;
}
.titleTable span{
	margin-right:30px;
	font: 18px 微软雅黑;
}
.a tr,.b tr{
	height:50px;
}
.a tbody span,.b tbody span{
	margin:0px;
	float:right;
}
.a tbody input,.b tbody input{
	width:280px;
	height:30px;
	line-height:30px;
	outline:none;
	padding-left:5px;
}
.a tbody textarea,.b tbody textarea{
	width:742px;
	height:80px;
	resize:none;
	outline:none;
	padding-left:5px;
}
.a tbody,.b tbody{
	font: 18px 微软雅黑;
}
.a .fspec span,.parentProduct .fspec span,.childProduct .fspec span{
	float:none;
	margin:0px 5px 0px 5px;
}
.a .fspec input{
	width:69.5px;
}
.character tr{
	height:35px;
}
.trBorder td{
	border:1px solid #bfbfbf;
}
.character .titleTr{
	background-color:#e5e5e5;
	border:1px solid #bfbfbf;
}
.a .character{
	border-collapse: collapse;
/* 	width:750px; */
	margin-left:135px;
	margin-top:10px;
	text-align:center;
}
.childProduct{
	display:none;
}
.parentProduct,.childProduct{
	width:750px;
	padding:0px;
	margin:0px;
}
.b .parentProduct table tr,.b .childProduct table tr{
	height:35px;
}
.parentProduct table,.childProduct table{
	width:760px;
	background-color:#e5e5e5;
	padding:10px;
/* 	margin-bottom:35px; */
}
.parentProduct table input,.childProduct table input{
	width:138px;
	height:25px;
	line-height:25px;
}
.parentProduct  .fspec input,.childProduct  .fspec input{
	width:40px;
	height:25px;
	line-height:25px;
}
.b .num input{
	width:60px;
	height:25px;
	line-height:25px;
}
.b .parentProduct  table a,.b .childProduct  table a{
	font:21px 微软雅黑;
	padding:5px;
	cursor: pointer;
}
.a,.b{
	display:none;
}
.delOrder{
    font: 18px 微软雅黑;
    display: inline-table;
    width: 78px;
    height: 21px;
    line-height: 21px;
    border: 1px solid #999;
    margin-right: 5px!important;
    cursor: pointer;
    background-color: #fff;
    /* color: #545454; */
    text-align: center;
}
.delOrder:hover{
	color:#fff;
	background-color:red;
}
.unselect {
    -webkit-user-select: none; 
    -moz-user-select: none;    
    -khtml-user-select: none;  
    -ms-user-select: none;    

    /* 以下两个属性目前并未支持，写在这里为了减少风险 */
    -o-user-select: none;
    user-select: none;  
}
.trBorder .editTd {
	width:243px;
	height:30px;
	line-height:30px;
	outline:none;
	padding-left:5px;
	border:none;
    text-align: center;
}
.trBorder td.delTd{
	border:none;
}
.delTr{
    margin: 0px 0 0 -10px;
    position: relative;
    background: url(../../../css/images/del.png) no-repeat scroll 0 0 transparent;
    width: 20px;
    float: right;
    height: 20px;
    overflow: hidden;
    text-indent: -9999px;
    top: -15px;
    right: 0px;
    display:none;
}

/* 上传文件样式 */
.webuploader-container .webuploader-pick{
height:50px;width:50px;left:0px;padding:0 0 0 0;line-height:43px;color: #a0a0a0;background: #e5e5e5;float:left;
}
.for_date {
    display: block;
    float: left;
    height: 30px;
    padding-top: 2px;
    width: 28px;
    border: 1px solid #A9A9A9;
    border-left: none;
    cursor: default;
}
.a tbody .date{
	width:254px;
	height:32px;
	line-height:32px;
	float:left;
	border:1px solid #a9a9a9;
	border-right:none;
}
.button span{
    font: 18px 微软雅黑;
    display: inline-table;
    width: 125px;
    height: 45px;
    line-height: 45px;
    border: 1px solid #999;
    margin-right: 5px!important;
    cursor: pointer;
/*     background-color: #fff; */
    text-align: center;
    border-radius: 4px;
    margin-bottom:10px;
}
.infoafter {
    background: #ffb155;
    display: inline-block;
    padding: 2px 22px;
    position: relative;
    overflow: visible;
    font-size: 12px;
    height: auto;
    width: auto;
}

.b .itemafter .infoafter a.delfile,.a .itemafter .infoafter a.delfile,.content .itemafter .infoafter a.delfile{
    position: absolute;
    width: 16px;
    height: 16px;
    line-height: 16px;
    right: -7px;
    top: -7px;
    border-radius: 50%;
    background: #d80c18;
    color: #fff;
    text-align: center;
    display: none;
    text-decoration: blink;
    padding:0px;
    font-size:16px;
}
.itemafter .infoafter:hover a,.a .itemafter .infoafter:hover a.delfile,.content .itemafter .infoafter:hover a.delfile,.b .itemafter .infoafter:hover a.delfile{
 	display: inline-block; 
 } 