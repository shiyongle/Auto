package com.model.menu;

/**
 * 左侧菜单表
 *
 */
public class Menu {
	private String fid;//菜单编号
	private String fname;//菜单
	private String furl;//菜单路径，调用的方法
	private Integer forder;//菜单排序
	
	
	public String getFid() {
		return fid;
	}
	public void setFid(String fid) {
		this.fid = fid;
	}
	public String getFname() {
		return fname;
	}
	public void setFname(String fname) {
		this.fname = fname;
	}
	public String getFurl() {
		return furl;
	}
	public void setFurl(String furl) {
		this.furl = furl;
	}
	
	
	public Integer getForder() {
		return forder;
	}
	public void setForder(Integer forder) {
		this.forder = forder;
	}
	public Menu(String fname, String furl, Integer forder) {
		super();
		this.fname = fname;
		this.furl = furl;
		this.forder = forder;
	}
	public Menu(String fid, String fname, String furl, Integer forder) {
		super();
		this.fid = fid;
		this.fname = fname;
		this.furl = furl;
		this.forder = forder;
	}
	public Menu() {
		super();
	}
	
	
}
