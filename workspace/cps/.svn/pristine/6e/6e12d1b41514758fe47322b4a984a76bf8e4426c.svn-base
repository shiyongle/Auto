<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.model.invite">
	<class name="Invite" table="t_sys_invite">
		<id name="fid" type="string">
            <column name="FID" length="36" />
            <generator class="assigned" />
        </id>
        <property name="fuserid" type="string"><column name="FUSERID" length="80" not-null="true" /></property>
        <property name="fuserroleid" type="int"><column name="FUSERROLEID" length="80" not-null="true" /></property>
        <property name="finviteeid" type="string" ><column name="FINVITEEID" length="80" not-null="true" /></property>
        <property name="fcreatetime" type="timestamp"><column name="FCREATETIME" length="0" /></property>
        <property name="fbonus" type="string"><column name="FBONUS" length="80"  /></property>
        <property name="fusername" type="string"><column name="FUSERNAME" length="80"  /></property>
        <property name="finviteename" type="string"><column name="FINVITEENAME" length="80"  /></property>
        <property name="fapptype" type="int"><column name="FAPPTYPE"  /></property>
	</class>
</hibernate-mapping>
