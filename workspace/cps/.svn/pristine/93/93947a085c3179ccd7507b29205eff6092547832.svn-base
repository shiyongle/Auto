package com.model.user;

import java.io.Serializable;
import java.util.Date;
/**
 * 登录用户
 */
public class TSysUser implements Serializable{
	private static final long serialVersionUID = 1L;
	/**** 主键*/
	private String fid;
	/**** 用户名称（账户）*/
	private String fname;
	/**** 密码*/
	private String fpassword;
	/**** 是否启用、禁用	1禁用0启用*/
	private int feffect=0;
	/**** 客户名称*/
	private String fcustomername;
	/**** 邮件*/
	private String femail;
	/**** 手机*/
	private String ftel;
	/**** 创建时间*/
	private Date fcreatetime;
	/**** 客户FID-外键*/
	private String fcustomerid;
	/**** 制造商FID-外键*/
	private String fsupplierid;
	/**** 是否查看所有客户-1是0否*/
	private int fisfilter = 0;
	/**** 只看自己帐号-1是0否*/
	private int fisreadonly=0;
	/**** 用户类型	0：客户1制造商2平台3设计师*/
	private Integer ftype = 0;
	/**** IM的记录号-IM沟通系统*/
	private Integer fimid;
	/**** 电话*/
	private String fphone;
	/**** 传真*/
	private String ffax;
	/**** 状态：0已登陆1未登陆*/
	private int fstate = 1;
	private String fqq="";
	/**父账户的ID*/
	private String fparentid;
	
	//验证码
	private String identCode;
	//旧密码
	private String foldpassword;
	
	public String getFoldpassword() {
		return foldpassword;
	}
	public void setFoldpassword(String foldpassword) {
		this.foldpassword = foldpassword;
	}
	public String getFid() {
		return fid;
	}
	public void setFid(String fid) {
		this.fid = fid;
	}
	public String getFname() {
		return fname;
	}
	public void setFname(String fname) {
		this.fname = fname;
	}
	public String getFpassword() {
		return fpassword;
	}
	public void setFpassword(String fpassword) {
		this.fpassword = fpassword;
	}
	public int getFeffect() {
		return feffect;
	}
	public void setFeffect(int feffect) {
		this.feffect = feffect;
	}
	public String getFcustomername() {
		return fcustomername;
	}
	public void setFcustomername(String fcustomername) {
		this.fcustomername = fcustomername;
	}
	public String getFemail() {
		return femail;
	}
	public void setFemail(String femail) {
		this.femail = femail;
	}
	public String getFtel() {
		return ftel;
	}
	public void setFtel(String ftel) {
		this.ftel = ftel;
	}
	public Date getFcreatetime() {
		return fcreatetime;
	}
	public void setFcreatetime(Date fcreatetime) {
		this.fcreatetime = fcreatetime;
	}
	public String getFcustomerid() {
		return fcustomerid;
	}
	public void setFcustomerid(String fcustomerid) {
		this.fcustomerid = fcustomerid;
	}
	public String getFsupplierid() {
		return fsupplierid;
	}
	public void setFsupplierid(String fsupplierid) {
		this.fsupplierid = fsupplierid;
	}
	public int getFisfilter() {
		return fisfilter;
	}
	public void setFisfilter(int fisfilter) {
		this.fisfilter = fisfilter;
	}
	public int getFisreadonly() {
		return fisreadonly;
	}
	public void setFisreadonly(int fisreadonly) {
		this.fisreadonly = fisreadonly;
	}
	public Integer getFtype() {
		return ftype;
	}
	public void setFtype(Integer ftype) {
		this.ftype = ftype;
	}
	public Integer getFimid() {
		return fimid;
	}
	public void setFimid(Integer fimid) {
		this.fimid = fimid;
	}
	public String getFphone() {
		return fphone;
	}
	public void setFphone(String fphone) {
		this.fphone = fphone;
	}
	public String getFfax() {
		return ffax;
	}
	public void setFfax(String ffax) {
		this.ffax = ffax;
	}
	public int getFstate() {
		return fstate;
	}
	public void setFstate(int fstate) {
		this.fstate = fstate;
	}
	public String getIdentCode() {
		return identCode;
	}
	public void setIdentCode(String identCode) {
		this.identCode = identCode;
	}
	public String getFqq() {
		return fqq;
	}
	public void setFqq(String fqq) {
		this.fqq = fqq;
	}
	public String getFparentid() {
		return fparentid;
	}
	public void setFparentid(String fparentid) {
		this.fparentid = fparentid;
	}
	
}
