@charset "utf-8";
/* CSS Document */
/* --------------------初始-------------------- */
*{margin:0px;padding:0px;}
ul{list-style:none;}
img{border:none;}
a{text-decoration: none;color:#000;font-size:12px;font-family:宋体;}
body{font-size:14px;background:#fff;font-family:微软雅黑;}
.noselect{
	moz-user-select: -moz-none;
	-moz-user-select: none;
	-o-user-select:none;
	-khtml-user-select:none;
	-webkit-user-select:none;
	-ms-user-select:none;
	user-select:none;
}
.clear{clear:both;font-size: 0px;}
.fl{float: left;}
.fr{float: right;}

/* --------------------开头导航------------------ */
a.download{font-size:12px;color:#00f;padding-left:6px;}
label{color:#4c4b4b;}
#header{background:#f0f0f0;width:1050px;height:42px;}
#header p{float:left;line-height:42px;padding-left:14px;}

/* --------------------主体-------------------- */
#main,#header,.product,#operate{margin: 0 auto;}
#main{width:946px;font-family:微软雅黑;padding: 0 42px;color: #343434;}
h1{height:44px;line-height:44px;background:url(../../../css/images/title_line2.gif) no-repeat center 20px;text-indent:156px;font-size:25px;color:#676464;}
.cust_sup{padding-top: 60px;}
.cust_sup label{font-size: 18px;}
.cust_sup input{width: 288px;height: 27px;line-height: 27px;text-indent: 5px;}
.customer{margin-right: 72px;}
.ope_add{width:148px;height:52px;font-size: 18px;margin-top: 10px;position: relative;}
.ope_add .plus{width: 50px;height: 50px;line-height: 43px;font-size: 60px;color: #a0a0a0;text-align:center;border: 1px dashed #a0a0a0;position: absolute;top: 0;right: 0;cursor: pointer;border-radius: 5px;}
.product{width: 804px;border: 1px solid #b5b5b5;margin-top: 17px;background: #e5e5e5;padding: 28px 22px 18px;font-size: 18px;position: relative;}
.product input{height: 27px;line-height:27px;border: 1px solid #b5b5b5;font-size: 18px;}
.product_info{margin-bottom: 16px;}
.spec{display: inline-block;}
.spec input{width: 52px;text-align: center;}
.name,.material,.fnumber{width: 170px;text-indent: 5px;}
.name,.height{margin-right: 14px;}
.file_label{letter-spacing: 6px;}
.input_group{display: inline-block;}
.amount{width: 58px;text-align:center;vertical-align: middle;ime-mode:disabled;}
.input_group span{display:inline-block;cursor:pointer;width:26px;height:28px;line-height:28px;text-align:center;vertical-align: middle;font-size: 33px;}
.input_group span:hover{color:#fff;background:#d80c18;}
.input_group span:active{color:#ccc;}
.minus{border-radius:4px 0 0 4px;}
.plus{border-radius:0 4px 4px 0;}
.thelist {padding: 24px 0 0 14px;color: #fff;width: 640px;} 
 
.file .itemafter .infoafter{background: #ffb155;border: 1px solid #f39800;border-radius: 12px;line-height: 20px;display: inline-block;padding: 2px 22px;position: relative;overflow:visible;}
 
/* .file li{margin: 0 8px 14px;height: 20px;line-height: 20px;display: inline-block;background: #ffb155;border: 1px solid #f39800;padding: 2px 22px;position: relative;border-radius: 12px;}
 */
.file p a.delfile{position: absolute;width:16px;height:16px;line-height: 18px;right: -7px;top: -7px;border-radius: 50%;background: #d80c18;color: #fff;text-align: center;display: none;_display:block;}
.file p:hover a.delfile{display: block;}
.file p a.delfile:active{color: #888;}  
.delete{position: absolute;width: 20px;height: 20px;line-height: 20px;font-size: 16px;color: #fff;background: #a5a5a5;border-radius: 50%;right: -10px;top: -10px;text-align: center;display: none;_display:block;}
 .product:hover .delete{display: block;}
 .parentProduct .delete{display: none!important;}
.product .delete:active{color: #ccc;}
#operate{width: 312px;height: 48px;margin-top: 40px;margin-bottom: 40px;}
#operate a{width: 127px;height: 48px;line-height: 48px;background: #f76350;border-radius: 4px;text-align: center;font-size: 20px;color: #fff;}
.webuploader-container .webuploader-pick{
height:50px;width:50px;left:0px;padding:0 0 0 0;line-height:43px;color: #a0a0a0;background: #e5e5e5;
}